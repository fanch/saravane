msgstr "isso só é preciso se você estiver em um subdomínio"

msgid "Most of the time it's not need"
msgstr "Na maioria das vezes não é preciso"

msgid "Start the service"
msgstr "Iniciar o serviço"

msgid "Checking the configuration, returning true or false"
msgstr "Verifique a toda a configuração, retornar true ou false"

msgid "Show the network configuration"
msgstr "Mostrar a configuração da rede"

msgid "Show the keyboard configuration"
msgstr "Mostrar a configuração do teclado"

msgid "Show the date and time settings"
msgstr "Mostrar a configuração de clock"

msgid "Show the locale and local time adjustment"
msgstr "Mostrar o local e ajuste de hora local"

