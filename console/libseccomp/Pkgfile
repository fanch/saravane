# Depends on: 
version=2.1.1
source=(
http://downloads.sourceforge.net/project/$name/$name-$version/$name-$version.tar.gz)


build(){
    cd $name-$version
    ./configure --prefix=/usr
    make
    make DESTDIR=$PKG install    
}



